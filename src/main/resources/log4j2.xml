<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="Console">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="ERROR">
      <AppenderRef ref="Console"/>
    </Root>
    <logger name="com.github.junlong" level="ERROR">
      <AppenderRef ref="Console"/>
    </logger>
    <!--
    <logger name="com.github.junlong" level="DEBUG">
      <AppenderRef ref="Console"/>
    </logger>
    <logger name="com.github.junlong" level="INFO">
      <AppenderRef ref="Console"/>
    </logger>
    <logger name="com.github.junlong" level="TRACE">
      <AppenderRef ref="Console"/>
    </logger>
    -->
  </Loggers>
</Configuration>
